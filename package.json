{
  "name": "sculptql",
  "version": "4.5.0",
  "type": "module",
  "bin": {
    "sculptql": "./dist/index.js"
  },
  "files": [
    "dist/index.js",
    ".next/standalone",
    ".env.example"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "perf:test": "node scripts/performance-test.js",
    "start": "NODE_ENV=production node server.js",
    "build:cli": "tsc -p tsconfig.cli.json && chmod +x dist/index.js",
    "build:prod": "NODE_ENV=production npm run build && npm run build:cli && cp -r .next/static .next/standalone/.next/static && cp -r public .next/standalone/public",
    "prepublishOnly": "npm run build:prod",
    "lint": "next lint",
    "test": "jest --no-cache",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:all": "npm run test && npm run test:e2e",
    "cypress:open": "cypress open",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "component": "cypress open --component",
    "component:headless": "cypress run --component"
  },
